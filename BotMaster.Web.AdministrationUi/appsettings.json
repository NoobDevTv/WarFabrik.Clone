{
  "Botmaster": {
    "PluginCreator": "TCPPluginCreator",
    "RunnersPath": "./runners",
    "PluginsPath": "./plugins"
  },
  "Debug": {
    "Wait": 20000
  },
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": true,
    "targets": {
      "file": {
        "type": "File",
        "fileName": "${basedir}/logs/DotNetRunner/${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "Microsoft.EntityFramework*",
        "maxLevel": "off",
        "final": true
      },
      {
        "logger": "Microsoft.AspNetCore.Components.RenderTree.*",
        "maxLevel": "off",
        "final": true
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "file"
      }
    ]
  }
}