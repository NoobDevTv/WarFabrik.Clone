{
  "Docker": {
    "Network": "",
    "DefaultNetwork": "nginx_proxy"
  },
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": true,
    "targets": {
      "file": {
        "type": "File",
        "fileName": "${basedir}/Logs/DockerRunner/${shortdate}.log"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "Microsoft.EntityFramework*",
        "maxLevel": "off",
        "final": true
      },
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "file"
      }
    ]
  }
}